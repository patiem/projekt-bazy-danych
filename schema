class Clients:
    ClientID
    ClientName (PK)
    IsCompany
    

class Companies:
    ClientName = FK(Klient)(PK)
    CompanyName
    NumberOfParticipants
    Email


class Participants:
    ParticipantID
    ClientID = FK(Clients)(PK)
    FirstName
    LastName
    Email
    Company
    StudentID


class Conferences:
    ConferenceID (PK)
    ConferenceName
    NumberOfSeats
    StartDate
    EndDate


Class Dni_konferencji:
   ID (PK)
   ...


class Workshops:
    WorkshopID (PK)
    ConferenceID = FK(Conferences)
    NumberOfSeats
    StartDate
    EndDate
    Price


class RegistrationsForConferences:
    conference = FK(Conferences)
    uczestnik = FK(Uczestnik)
    paid_at
    StartDate
    EndDate (dla konferencji jednodniowej start_date == end_date)


Class RegistrationsForWorkshops:
    WorkshopID = FK(Workshops)
    ParticipantID = FK(Participants)
    PaidAt


Class ConferencePriceThreshold:
    ConferenceID = FK(Conferences)
    StartDate (data, od kt√≥rej ta cena jest aplikowana)
    Price
    StudentDiscount


Class Lecturer
    LecturerID (PK)
    Name

