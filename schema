class Clients:
    ClientName (PK)
    IsCompany
    

class Companies:
    ClientName = FK(Klient)(PK)
    CompanyName
    NumberOfParticipants
    Email


class Uczestnik:
    ClientName = FK(Klient)(PK)
    FirstName
    LastName
    Email
    Company
    StudentID


class Conferences:
    ID (PK)
    ConferenceName
    number_of_seats
    Start_date
    End_date


Class Dni_konferencji:
   ID (PK)
   ...


class Workshop:
    ID (PK)
    ConferenceID = FK(Conferences)
    number_of_seats
    start_date
    end_date
    price


class RejestracjaNaKonferencję: // lub dzień konferencji
    konferencja = FK(Konferencja)
    uczestnik = FK(Uczestnik)
    paid_at
    start_date (dla konferencji jednodniowej start_date == end_date)
    end_date


Class RejestracjaNaWarsztat:
    warsztat = FK(Warsztat)
    uczestnik = FK(Uczestnik)
    paid_at


Class ProgCenowyNaKonferencje:
    konferencja = FK(Konferencja)
    start_date (data, od której ta cena jest aplikowana)
    price
    Student_discount


Class Lecturer
    ID (PK)
    Name

