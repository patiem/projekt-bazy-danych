Clients:
    ClientID
    ClientName (PK)
    Email
    IsCompany


Participants:
    ParticipantID
    ClientID = FK(Clients)(PK)
    FirstName
    LastName
    Email
    StudentID


Conferences:
    ConferenceID (PK)
    ConferenceName
    NumberOfSeats
    StartDate
    EndDate


Workshops:
    WorkshopID (PK)
    ConferenceID = FK(Conferences)
    NumberOfSeats
    StartDateTime
    EndDateTime
    Price


// jeśli rejestracja jest np na dzień 11/12/17, 12/12/17, 13/12/17 i 15/12/17
// to robimy dla tej rejestarcji 2 obiekty RegistrationDateRange:
// 1) StartDate = 11/12/17, EndDate = 13/12/17
// 2) StartDate = 15/12/17, EndDate = 15/12/17
RegistrationDateRanges:
    RegistrationForConferenceID = FK(RegistrationsForConferences)
    StartDate
    EndDate


RegistrationsForConferences:
    RegistrationForConferenceID
    ConferenceID = FK(Conferences)
    ParticipantID = FK(Participants)
    PaidAt
    // daty na podstawie RegistrationDateRanges


RegistrationsForWorkshops:
    WorkshopID = FK(Workshops)
    ParticipantID = FK(Participants)
    PaidAt


ConferencePriceThresholds:
    ConferenceID = FK(Conferences)
    StartDate   // data, od której ta cena jest aplikowana
    Price
    StudentDiscount


Lecturers:
    LecturerID (PK)
    Name

